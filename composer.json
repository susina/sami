{
    "name": "sami/sami",
    "type": "application",
    "description": "Sami, an API documentation generator",
    "keywords": ["phpdoc"],
    "homepage": "http://sami.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "pimple/pimple": "^3.0",
        "twig/twig": "^3.0",
        "nikic/php-parser": "^4.0",
        "michelf/php-markdown": "^1.3",
        "symfony/console": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/process": "^5.0",
        "blackfire/php-sdk": "^1.5.18",
        "gossi/docblock": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Sami\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sami\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "prefer-stable": true,
    "bin": ["sami.php"]
}
